<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_chat"
        android:name="edu.uw.tcss450.Team4.TCSS450Project.ui.chat.ChatFragment"
        android:label="Chat"
        tools:layout="@layout/fragment_chat" />

    <fragment
        android:id="@+id/navigation_weather"
        android:name="edu.uw.tcss450.Team4.TCSS450Project.ui.weather.WeatherFragment"
        android:label="Weather"
        tools:layout="@layout/fragment_weather" />

    <fragment
        android:id="@+id/navigation_contacts"
        android:name="edu.uw.tcss450.Team4.TCSS450Project.ui.contacts.ContactsFragment"
        android:label="Contacts"
        tools:layout="@layout/fragment_contacts_list" >
        <action
            android:id="@+id/action_navigation_contacts_to_navigation_chat"
            app:destination="@id/navigation_chat" />
        <action
            android:id="@+id/action_navigation_contacts_to_createNewContactActivity"
            app:destination="@id/createNewContactActivity" />
        <action
            android:id="@+id/action_navigation_contacts_to_contactsProfile"
            app:destination="@id/contactsProfile" />
    </fragment>

    <fragment
        android:id="@+id/navigation_home"
        android:name="edu.uw.tcss450.Team4.TCSS450Project.ui.homeLanding.HomeLandingFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home_landing" />
    <activity
        android:id="@+id/createNewContactActivity"
        android:name="edu.uw.tcss450.Team4.TCSS450Project.ui.contacts.CreateNewContactActivity"
        android:label="CreateNewContactActivity" />
    <fragment
        android:id="@+id/contactsProfile"
        android:name="edu.uw.tcss450.Team4.TCSS450Project.ui.contacts.ContactsProfile"
        android:label="ContactsProfile" />

</navigation>