<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainActivity">

    <activity
        android:id="@+id/mainActivity"
        android:name="edu.uw.tcss450.Team4.TCSS450Project.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" >
        <argument android:name="email"
            app:argType="string" />
        <argument android:name="password" />
    </activity>
    <fragment
        android:id="@+id/registrationFragment"
        android:name="edu.uw.tcss450.Team4.TCSS450Project.registration.RegistrationFragment"
        android:label="fragment_registration"
        tools:layout="@layout/fragment_registration" >
        <action
            android:id="@+id/action_registrationFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
        <action
            android:id="@+id/action_registrationFragment_to_mainActivity2"
            app:destination="@id/mainActivity" />
    </fragment>
</navigation>